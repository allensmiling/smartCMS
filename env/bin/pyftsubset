#!/usr/home/allen/work/smartCMS/env/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'fonttools==3.15.1','console_scripts','pyftsubset'
__requires__ = 'fonttools==3.15.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('fonttools==3.15.1', 'console_scripts', 'pyftsubset')()
    )
